sed -i 's/@OPENSHIFT_DIY_IP@/'"$OPENSHIFT_DIY_IP"'/g' $OPENSHIFT_DIY_DIR/.openshift/biblio.openshift.yml
sed -i 's/@OPENSHIFT_DIY_PORT@/'"$OPENSHIFT_DIY_PORT"'/g' $OPENSHIFT_DIY_DIR/.openshift/biblio.openshift.yml

nohup java -jar biblio-server-rest.jar server $OPENSHIFT_DIY_DIR/.openshift/biblio.openshift.yml > ${OPENSHIFT_DIY_LOG_DIR}/biblio.log 2>&1 &

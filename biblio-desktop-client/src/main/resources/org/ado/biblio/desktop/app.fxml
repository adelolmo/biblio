<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<SplitPane dividerPositions="0.35566188197767146, 0.3923444976076555, 0.5" orientation="VERTICAL" prefHeight="629.0"
           prefWidth="928.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="org.ado.biblio.desktop.AppPresenter">
    <items>
        <MenuBar>
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#linkDropbox" text="Dropbox..."/>
                        <MenuItem mnemonicParsing="false" text="Close"/>
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Delete"/>
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                    <items>
                        <MenuItem mnemonicParsing="false" text="About"/>
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE">
            <tabs>
                <Tab text="Library">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <children>
                                <TableView fx:id="tableViewBooks" editable="true" layoutX="366.0" layoutY="10.0"
                                           prefHeight="204.0" prefWidth="926.0" AnchorPane.bottomAnchor="0.0"
                                           AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                                           AnchorPane.topAnchor="0.0">
                                    <columns>
                                        <TableColumn fx:id="tableColumnTitle" editable="false" prefWidth="367.0"
                                                     text="Title"/>
                                        <TableColumn fx:id="tableColumnAuthor" minWidth="0.0" prefWidth="361.0"
                                                     text="Author"/>
                                    </columns>
                                </TableView>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab text="Lend">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <children>
                                <TableView layoutX="363.0" layoutY="2.0" prefHeight="204.0" prefWidth="926.0"
                                           AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                           AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                    <columns>
                                        <TableColumn prefWidth="75.0" text="C1"/>
                                        <TableColumn prefWidth="75.0" text="C2"/>
                                    </columns>
                                </TableView>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab text="History">
                    <content>
                        <TableView prefHeight="200.0" prefWidth="200.0">
                    <columns>
                        <TableColumn prefWidth="75.0" text="C1"/>
                        <TableColumn prefWidth="75.0" text="C2"/>
                    </columns>
                        </TableView>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
        <AnchorPane prefHeight="200.0" prefWidth="200.0">
            <children>
                <HBox layoutX="712.0">
                    <children>
                        <ImageView fx:id="imageViewCover" fitHeight="322.0" fitWidth="200.0" layoutX="712.0"
                                   pickOnBounds="true" preserveRatio="true"/>
                    </children>
                </HBox>
                <HBox layoutX="13.9619140625" layoutY="9.0">
                    <children>
                        <Label layoutX="14.0" layoutY="14.0" text="Title:"/>
                        <TextField fx:id="textFieldTitle" layoutX="62.0" layoutY="9.0" prefHeight="25.0"
                                   prefWidth="651.0"/>
                    </children>
                </HBox>
                <HBox layoutX="14.0" layoutY="40.0">
                    <children>
                        <Label layoutX="14.0" layoutY="37.0" text="Author:"/>
                        <TextField fx:id="textFieldAuthor" layoutX="62.0" layoutY="32.0" prefHeight="25.0"
                                   prefWidth="353.0"/>
                    </children>
                </HBox>
                <HBox layoutX="14.0" layoutY="72.0">
                    <children>
                        <Label layoutX="14.0" layoutY="65.0" text="ISBN:"/>
                        <TextField fx:id="textFieldIsbn" layoutX="50.0" layoutY="60.0"/>
                    </children>
                </HBox>
                <ImageView fx:id="imageViewQrCode" fitHeight="150.0" fitWidth="200.0" layoutX="18.0" layoutY="172.0"
                           pickOnBounds="true" preserveRatio="true"/>
            </children>
        </AnchorPane>
        <Label fx:id="labelSystem" prefHeight="15.0" prefWidth="405.0"/>
    </items>
</SplitPane>
